<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.carlos.smartcar.settings_help.Quienesomos">

    <WebView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/webview"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/textView5" />

    <TextView
        android:text="Smart Car es un proyecto de fin de grado llevado por estudiantes del Centre d'Estudis Politècnics, és una idea firme que lleva con nosotros desde hace un par de años.   Queremos que este proyecto salga adelante y esperamos que algún día pueda salir al mercado y ser algo más que un mero trabajo de clase.   ¡Deseadnos suerte!"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView5"
        android:textColor="@color/colorPrimary"
        android:textSize="16sp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:paddingBottom="15dp" />
</RelativeLayout>
